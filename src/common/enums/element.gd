@abstract
extends RefCounted
class_name Element

enum Type {
	BLUNT,
	PIERCE,
	SLASH,
	FIRE,
	ICE,
	LIGHTNING,
	WIND,
	TRUE
}

const BLUNT := Type.BLUNT
const PIERCE := Type.PIERCE
const SLASH := Type.SLASH
const FIRE := Type.FIRE
const ICE := Type.ICE
const LIGHTNING := Type.LIGHTNING
const WIND := Type.WIND
const TRUE := Type.TRUE
